= form_with model: @address, local: true do |f|
  = f.label '郵便番号'
  = f.text_field :postcode
  %br
  = f.label '県'
  = f.text_field :prefecture
  %br
  = f.label '市区町村'
  = f.text_field :city
  %br
  = f.label '丁目・番地'
  = f.text_field :address1
  %br
  = f.label 'ビル名・アパート名など'
  = f.text_field :address2
  %br
  = f.submit '登録', class: 'btn btn-primary'